# Environment Variables for Production Deployment

# Application Configuration
NODE_ENV=production
PORT=8080

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# API Configuration
VITE_API_BASE_URL=https://your-api-domain.com
VITE_API_TIMEOUT=30000

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true

# Third-party Services
VITE_TAVUS_API_KEY=your-tavus-api-key-here

# Security Configuration
VITE_APP_DOMAIN=your-app-domain.com
VITE_ALLOWED_ORIGINS=https://your-app-domain.com,https://www.your-app-domain.com

# Monitoring and Logging
VITE_LOG_LEVEL=error
VITE_SENTRY_DSN=your-sentry-dsn-here

# Google Cloud Configuration (for Cloud Run)
GOOGLE_CLOUD_PROJECT=your-project-id
GOOGLE_CLOUD_REGION=us-central1

# Build Configuration
VITE_BUILD_VERSION=1.0.0
VITE_BUILD_TIMESTAMP=2024-01-01T00:00:00Z